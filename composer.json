{
    "name": "rockberpro/rosa-router",
    "description": "Smart REST router for PHP",
    "license": "MIT",
    "config": {
        "platform": {
            "php": "7.4.3"
        }
    },
    "require": {
        "php": ">=7.4",
        "react/http": "^1.11",
        "monolog/monolog": "^2.10",
        "symfony/http-foundation": "^5.4",
        "symfony/dotenv": "3.4",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Rockberpro\\RestRouter\\": "src/",
            "Rockberpro\\RestRouter\\Controllers\\": "controllers/",
            "Rockberpro\\RestRouter\\Routes\\": "routes/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symfony/var-dumper": "^5.4"
    }
}
