    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "react/http": "^1.11",
        "monolog/monolog": "^2.10",
        "symfony/http-foundation": "^7.3",
        "symfony/dotenv": "3.4",
        "ext-json": "*"
    },
    "autoload": {
